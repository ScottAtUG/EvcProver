<UserControl x:Class="Prover.GUI.Views.InstrumentReport.InstrumentReportView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Prover.GUI.Views.InstrumentReport"
             xmlns:cal="http://www.caliburnproject.org"
             mc:Ignorable="d" 
             d:DesignHeight="816" d:DesignWidth="1056">
    <Viewbox Width="795" Height="1040" Stretch="UniformToFill">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="0.5*"/>
                <RowDefinition Height="0.75*"/>
                <RowDefinition Height="1*"/>
            </Grid.RowDefinitions>
            <ContentControl Grid.Row="0" cal:View.Model="{Binding SiteInformation}"></ContentControl>
            <ContentControl Grid.Row="1" cal:View.Model="{Binding Temperature}" ></ContentControl>
            <ContentControl Grid.Row="2" cal:View.Model="{Binding Volume}"></ContentControl>
        </Grid>
    </Viewbox>
</UserControl>
